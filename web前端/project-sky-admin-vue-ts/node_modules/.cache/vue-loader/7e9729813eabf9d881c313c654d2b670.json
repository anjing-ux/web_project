item label=\"备注:\">\r\n            <el-input v-model=\"ruleForm.remark\" type=\"textarea\" :rows=\"3\" maxlength=\"200\" placeholder=\"备注，最长200字\"\r\n              :disabled=\"isDisabled('remark')\" />\r\n          </el-form-item>\r\n        </div>\r\n        <!-- 采购日期 -->\r\n        <div>\r\n          <el-form-item label=\"采购日期:\" prop=\"purchaseDate\">\r\n            <el-date-picker v-model=\"ruleForm.purchaseDate\" type=\"datetime\" placeholder=\"请选择采购日期\"\r\n              value-format=\"yyyy-MM-dd HH:mm:ss\" :disabled=\"isDisabled('purchaseDate')\" />\r\n          </el-form-item>\r\n        </div>\r\n        <!-- 采购金额 -->\r\n        <div>\r\n          <el-form-item label=\"采购金额:\" prop=\"purchaseAmount\">\r\n            <el-input v-model=\"ruleForm.purchaseAmount\" placeholder=\"请输入采购金额\"\r\n              :disabled=\"isDisabled('purchaseAmount')\" />\r\n          </el-form-item>\r\n        </div>\r\n        <!-- 采购单据编号 -->\r\n        <div>\r\n          <el-form-item label=\"采购单据编号:\" prop=\"purchaseDocumentNo\">\r\n            <el-input v-model=\"ruleForm.purchaseDocumentNo\" placeholder=\"请输入采购单据编号\"\r\n              :disabled=\"isDisabled('purchaseDocumentNo')\" />\r\n          </el-form-item>\r\n        </div>\r\n        <!-- 合同编号 -->\r\n        <div>\r\n          <el-form-item label=\"合同编号:\" prop=\"contractNo\">\r\n            <el-input v-model=\"ruleForm.contractNo\" placeholder=\"请输入合同编号\" :disabled=\"isDisabled('contractNo')\" />\r\n          </el-form-item>\r\n        </div>\r\n        <!-- 采购负责人 -->\r\n        <div>\r\n          <el-form-item label=\"采购负责人:\" prop=\"purchasePerson\">\r\n            <el-input v-model=\"ruleForm.purchasePerson\" placeholder=\"请输入采购负责人\"\r\n              :disabled=\"isDisabled('purchasePerson')\" />\r\n          </el-form-item>\r\n        </div>\r\n        <!-- 采购设备图像 -->\r\n       <div>\r\n          <el-form-item label=\"采购设备图像:\" prop=\"image\">\r\n            <el-upload v-if=\"!ruleForm.image\" class=\"avatar-uploader\" action=\"/api/common/upload\" :show-file-list=\"false\"\r\n              :on-success=\"handleImageSuccess\" :before-upload=\"beforeImageUpload\"\r\n              :headers=\"headers\">\r\n              <i class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n            </el-upload>\r\n            <div v-else class=\"image-container\">\r\n              <el-image \r\n                :src=\"ruleForm.image\" \r\n                class=\"avatar\" \r\n                :preview-src-list=\"[ruleForm.image]\" \r\n                fit=\"cover\">\r\n              </el-image>\r\n              <div class=\"image-actions\">\r\n                <el-button type=\"text\" icon=\"el-icon-refresh\" @click=\"resetImage\">重新上传</el-button>\r\n              </div>\r\n            </div>\r\n          </el-form-item>\r\n        </div>\r\n        <!-- 交付日期 -->\r\n        <div>\r\n          <el-form-item label=\"交付日期:\" prop=\"deliveryDate\">\r\n            <el-date-picker v-model=\"ruleForm.deliveryDate\" type=\"datetime\" placeholder=\"请选择交付日期\"\r\n              value-format=\"yyyy-MM-dd HH:mm:ss\" :disabled=\"isDisabled('deliveryDate')\" />\r\n          </el-form-item>\r\n        </div>\r\n        <!-- 验收日期 -->\r\n        <div>\r\n          <el-form-item label=\"验收日期:\" prop=\"acceptanceDate\">\r\n            <el-date-picker v-model=\"ruleForm.acceptanceDate\" type=\"datetime\" placeholder=\"请选择验收日期\"\r\n              value-format=\"yyyy-MM-dd HH:mm:ss\" :disabled=\"isDisabled('acceptanceDate')\" />\r\n          </el-form-item>\r\n        </div>\r\n        <!-- 验收结果 -->\r\n        <div>\r\n          <el-form-item label=\"验收结果:\" prop=\"acceptanceResult\">\r\n            <el-select v-model=\"ruleForm.acceptanceResult\" placeholder=\"请选择验收结果\"\r\n              :disabled=\"isDisabled('acceptanceResult')\">\r\n              <el-option label=\"合格\" value=\"1\" />\r\n              <el-option label=\"不合格\" value=\"2\" />\r\n              <el-option label=\"部分合格\" value=\"3\" />\r\n            </el-select>\r\n          </el-form-item>\r\n        </div>\r\n        <!-- 验收备注 -->\r\n        <div>\r\n          <el-form-item label=\"验收备注:\" prop=\"acceptanceNotes\">\r\n            <el-input v-model=\"ruleForm.acceptanceNotes\" type=\"textarea\" :rows=\"3\" maxlength=\"200\"\r\n              placeholder=\"验收备注，最长200字\" :disabled=\"isDisabled('acceptanceNotes')\" />\r\n          </el-form-item>\r\n        </div>\r\n        <!-- 催促通知 -->\r\n        <div>\r\n          <el-form-item label=\"催促通知:\" prop=\"promptMessage\">\r\n            <el-input v-model=\"ruleForm.promptMessage\" type=\"textarea\" :rows=\"3\" maxlength=\"200\"\r\n              placeholder=\"催促通知，最长200字\" :disabled=\"isDisabled('promptMessage')\" />\r\n          </el-form-item>\r\n        </div>\r\n        <!-- 创建时间 -->\r\n        <div>\r\n          <el-form-item label=\"创建时间:\" prop=\"createTime\">\r\n            <el-date-picker v-model=\"ruleForm.createTime\" type=\"datetime\" placeholder=\"请选择创建时间\"\r\n              value-format=\"yyyy-MM-dd HH:mm:ss\" :disabled=\"isDisabled('createTime')\" />\r\n          </el-form-item>\r\n        </div>\r\n        <!-- 创建人 -->\r\n        <div>\r\n          <el-form-item label=\"创建人:\" prop=\"createBy\">\r\n            <el-input v-model=\"ruleForm.createBy\" placeholder=\"请输入创建人\" :disabled=\"isDisabled('createBy')\" />\r\n          </el-form-item>\r\n        </div>\r\n        <!-- 操作按钮 -->\r\n        <div class=\"subBox address\">\r\n          <el-form-item>\r\n            <el-button @click=\"() => $router.back()\">\r\n              取消\r\n            </el-button>\r\n            <el-button type=\"primary\" v-if=\"this.status == '3'\" @click=\"confirmPurchasingForm('ruleForm')\">\r\n              交付\r\n            </el-button>\r\n            <el-button type=\"primary\" v-if=\"this.status == '1'\" @click=\"submitForm('ruleForm')\">\r\n              提交审核\r\n            </el-button>\r\n          </el-form-item>\r\n        </div>\r\n      </el-form>\r\n    </div>\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport { Component, Vue } from 'vue-property-decorator'\r\nimport HeadLable from '@/components/HeadLable/index.vue'\r\nimport ImageUpload from '@/components/ImgUpload/index.vue'\r\nimport { confirmPurchasingForm, queryPurchaseDetailById, submitPurchasingForm } from '@/api/purchase'\r\nimport { status } from 'nprogress'\r\nimport { number } from 'echarts'\r\nimport { getToken } from '@/utils/cookies'\r\n\r\n\r\n@Component({\r\n  name: 'addShop',\r\n  components: {\r\n    HeadLable,\r\n    ImageUpload\r\n  }\r\n})\r\nexport default class extends Vue {\r\n  private value: string = ''\r\n  private purchaseId: string = ''\r\n  private status: string = ''\r\n  private initiallyNonEmptyFields: string[] = []; // 存储初始非空字段名\r\n  private ruleForm = {\r\n    purchaseId: '', // 采购ID\r\n    equipmentType: '', // 设备类型\r\n    equipmentName: '', // 设备名称\r\n    modelNumber: '', // 型号\r\n    specification: '', // 规格参数\r\n    manufacturer: '', // 生产厂家\r\n    supplier: '', // 供应商\r\n    purchaseDate: '', // 采购日期\r\n    purchaseAmount: '', // 采购金额\r\n    quantity: '', // 采购数量\r\n    unit: '', // 单位\r\n    warrantyPeriod: '', // 保修期(月)\r\n    expectedLife: '', // 预期使用寿命(月)\r\n    purchaseDocumentNo: '', // 采购单据编号\r\n    contractNo: '', // 合同编号\r\n    purchasePerson: '', // 采购负责人\r\n    image: '', // 采购设备图像\r\n    status: 0,\r\n    deliveryDate: '', // 交付日期\r\n    acceptanceDate: '', // 验收日期\r\n    acceptanceResult: '', // 验收结果\r\n    acceptanceNotes: '', // 验收备注\r\n    remark: '', // 备注\r\n    cancelReason: '', // 取消采购单原因\r\n    refuseReason: '', // 审核拒绝原因\r\n    promptMessage: '', // 催促通知\r\n    createTime: '', // 创建时间\r\n    createBy: '', // 创建人\r\n  }\r\n  private headers = {\r\n  token: getToken()\r\n}\r\n  get rules() {\r\n    return {\r\n      equipmentName: {\r\n        required: true,\r\n        validator: (rule, value, callback) => {\r\n          if (!value) {\r\n            callback(new Error('请输入设备名称'));\r\n          } else {\r\n            const reg = /^([A-Za-z0-9\\u4e00-\\u9fa5]){2,20}$/;\r\n            if (!reg.test(value)) {\r\n              callback(new Error('设备名称输入不符，请输入2-20个字符'));\r\n            } else {\r\n              callback();\r\n            }\r\n          }\r\n        },\r\n        trigger: 'blur'\r\n      },\r\n      equipmentType: {\r\n        required: true,\r\n        message: '请选择设备类型',\r\n        trigger: 'change'\r\n      },\r\n      modelNumber: {\r\n        required: true,\r\n        message: '请输入设备型号',\r\n        trigger: 'blur'\r\n      },\r\n      specification: {\r\n        required: true,\r\n        message: '请输入规格参数',\r\n        trigger: 'blur'\r\n      },\r\n      manufacturer: {\r\n        required: true,\r\n        message: '请输入生产厂家',\r\n        trigger: 'blur'\r\n      },\r\n      supplier: {\r\n        required: true,\r\n        message: '请输入供应商',\r\n        trigger: 'blur'\r\n      },\r\n      quantity: {\r\n        required: true,\r\n        validator: (rule, value, callback) => {\r\n          if (!value) {\r\n            callback(new Error('请输入采购数量'));\r\n          } else {\r\n            const reg = /^[1-9]\\d*$/; // 正整数\r\n            if (!reg.test(value)) {\r\n              callback(new Error('采购数量必须是正整数'));\r\n            } else {\r\n              callback();\r\n            }\r\n          }\r\n        },\r\n        trigger: 'blur'\r\n      },\r\n      unit: {\r\n        required: true,\r\n        message: '请输入单位',\r\n        trigger: 'blur'\r\n      },\r\n      warrantyPeriod: {\r\n        required: true,\r\n        validator: (rule, value, callback) => {\r\n          if (!value) {\r\n            callback();\r\n          } else {\r\n            const reg = /^[1-9]\\d*$/; // 正整数\r\n            if (!reg.test(value)) {\r\n              callback(new Error('保修期必须是正整数'));\r\n            } else {\r\n              callback();\r\n            }\r\n          }\r\n        },\r\n        trigger: 'blur'\r\n      },\r\n      expectedLife: {\r\n        required: true,\r\n        validator: (rule, value, callback) => {\r\n          if (!value) {\r\n            callback();\r\n          } else {\r\n            const reg = /^[1-9]\\d*$/; // 正整数\r\n            if (!reg.test(value)) {\r\n              callback(new Error('预期使用寿命必须是正整数'));\r\n            } else {\r\n              callback();\r\n            }\r\n          }\r\n        },\r\n        trigger: 'blur'\r\n      },\r\n      remark: {\r\n        required: true,\r\n        validator: (rule, value, callback) => {\r\n          if (value && value.length > 100) {\r\n            callback(new Error('备注不能超过100个字符'));\r\n          } else {\r\n            callback();\r\n          }\r\n        },\r\n        trigger: 'blur'\r\n      },\r\n      purchaseDate: {\r\n        required: true,\r\n        message: '请选择采购日期',\r\n        trigger: 'blur'\r\n      },\r\n      purchaseAmount: {\r\n        required: true,\r\n        validator: (rule, value, callback) => {\r\n          if (!value) {\r\n            callback();\r\n          } else {\r\n            const reg = /^[0-9]+(\\.[0-9]{1,2})?$/; // 正数且最多两位小数\r\n            if (!reg.test(value)) {\r\n              callback(new Error('采购金额必须是正数，最多两位小数'));\r\n            } else {\r\n              callback();\r\n            }\r\n          }\r\n        },\r\n        trigger: 'blur'\r\n      },\r\n      purchaseDocumentNo: {\r\n        required: true,\r\n        message: '请输入采购单据编号',\r\n        trigger: 'blur'\r\n      },\r\n      contractNo: {\r\n        required: true,\r\n        message: '请输入合同编号',\r\n        trigger: 'blur'\r\n      },\r\n      purchasePerson: {\r\n        required: true,\r\n        message: '请输入采购负责人',\r\n        trigger: 'blur'\r\n      },\r\n      deliveryDate: {\r\n        required: true,\r\n        message: '请选择交付日期',\r\n        trigger: 'blur'\r\n      },\r\n      acceptanceDate: {\r\n        required: true,\r\n        message: '请选择验收日期',\r\n        trigger: 'blur'\r\n      },\r\n      acceptanceResult: {\r\n        