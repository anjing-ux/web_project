;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA","file":"inbound.vue","sourceRoot":"src/views/WarehouseManager","sourcesContent":["<template>\r\n  <div class=\"dashboard-container\">\r\n    <div class=\"container\">\r\n      <div class=\"tableBar\">\r\n        <label style=\"margin-right: 10px\">设备名称：</label>\r\n        <el-input v-model=\"equipmentName\" placeholder=\"请填写设备名称\" style=\"width: 14%\" clearable @clear=\"init\"\r\n          @keyup.enter.native=\"initFun\" />\r\n        <label style=\"margin-right: 10px; margin-left: 20px\">设备类型：</label>\r\n        <el-select v-model=\"equipmentType\" style=\"width: 14%\" placeholder=\"请选择\" clearable @clear=\"init\">\r\n          <el-option label=\"客服设备\" value=\"1\" />\r\n          <el-option label=\"机电设备\" value=\"2\" />\r\n          <el-option label=\"消防设备\" value=\"3\" />\r\n          <el-option label=\"电梯设备\" value=\"4\" />\r\n        </el-select>\r\n        <el-button class=\"normal-btn continue\" @click=\"init(true)\">\r\n          查询\r\n        </el-button>\r\n        <!-- 新增批量入库按钮 -->\r\n        <el-button \r\n          class=\"normal-btn continue\" \r\n          :disabled=\"selectedDevices.length === 0\"\r\n          @click=\"showBatchInboundDialog\"\r\n          style=\"margin-left: 10px\">\r\n          批量入库\r\n        </el-button>\r\n      </div>\r\n      <el-table v-if=\"tableData.length\" :data=\"tableData\" stripe class=\"tableBox\"\r\n        @selection-change=\"handleSelectionChange\">\r\n        <el-table-column type=\"selection\" width=\"55\" />\r\n        <el-table-column prop=\"deviceId\" label=\"设备编号\" />\r\n        <el-table-column prop=\"equipmentName\" label=\"设备名称\" />\r\n        <el-table-column prop=\"equipmentType\" label=\"设备类型\" />\r\n        <el-table-column prop=\"modelNumber\" label=\"设备型号\" />\r\n        <el-table-column label=\"操作\" width=\"250\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"text\" size=\"small\" class=\"blueBug\" @click=\"deviceInbound(scope.row.deviceId)\">\r\n              入库\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <Empty v-else :is-search=\"isSearch\" />\r\n      <el-pagination v-if=\"counts > 10\" class=\"pageList\" :page-sizes=\"[10, 20, 30, 40]\" :page-size=\"pageSize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\" :total=\"counts\" @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\" />\r\n    </div>\r\n    \r\n    <!-- 单个入库对话框 -->\r\n    <el-dialog title=\"设备入库信息\" :visible.sync=\"dialogVisible\" width=\"50%\" @close=\"resetForm\">\r\n      <el-form :model=\"inboundForm\" label-width=\"120px\">\r\n        <el-form-item label=\"区域ID\" v-if=\"false\">\r\n          <el-input v-model=\"inboundForm.areald\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"设备编号\">\r\n          <el-input v-model=\"inboundForm.deviceId\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"入库操作人\" required>\r\n          <el-input v-model=\"inboundForm.inboundBy\" placeholder=\"请输入操作人姓名\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"检验日期\" required>\r\n          <el-date-picker v-model=\"inboundForm.inspectionDate\" type=\"date\" placeholder=\"选择检验日期\"\r\n            value-format=\"yyyy-MM-dd\">\r\n          </el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item label=\"检验结果\" required>\r\n          <el-select v-model=\"inboundForm.inspectionResult\" placeholder=\"请选择\">\r\n            <el-option label=\"优良\" value=\"优良\"></el-option>\r\n            <el-option label=\"合格\" value=\"合格\"></el-option>\r\n            <el-option label=\"不合格\" value=\"不合格\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"货架位置\" required>\r\n          <el-input v-model=\"inboundForm.storageLocation\" placeholder=\"例如：二仓\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"仓库ID\" v-if=\"false\">\r\n          <el-input v-model=\"inboundForm.warehouseId\" disabled></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"submitInbound\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n    \r\n    <!-- 批量入库对话框 -->\r\n    <el-dialog title=\"批量入库\" :visible.sync=\"batchDialogVisible\" width=\"50%\">\r\n      <el-form :model=\"batchInboundForm\" label-width=\"120px\">\r\n        <el-form-item label=\"入库操作人\" required>\r\n          <el-input v-model=\"batchInboundForm.inboundBy\" placeholder=\"请输入操作人姓名\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"检验日期\" required>\r\n          <el-date-picker v-model=\"batchInboundForm.inspectionDate\" type=\"date\" placeholder=\"选择检验日期\"\r\n            value-format=\"yyyy-MM-dd\">\r\n          </el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item label=\"检验结果\" required>\r\n          <el-select v-model=\"batchInboundForm.inspectionResult\" placeholder=\"请选择\">\r\n            <el-option label=\"优良\" value=\"优良\"></el-option>\r\n            <el-option label=\"合格\" value=\"合格\"></el-option>\r\n            <el-option label=\"不合格\" value=\"不合格\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"货架位置\" required>\r\n          <el-input v-model=\"batchInboundForm.storageLocation\" placeholder=\"例如：二仓\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"待入库设备\">\r\n          <el-table :data=\"selectedDevices\" border style=\"width: 100%\">\r\n            <el-table-column prop=\"deviceId\" label=\"设备编号\"></el-table-column>\r\n            <el-table-column prop=\"equipmentName\" label=\"设备名称\"></el-table-column>\r\n            <el-table-column prop=\"equipmentType\" label=\"设备类型\"></el-table-column>\r\n          </el-table>\r\n          <div style=\"margin-top: 10px; text-align: right;\">\r\n            共 {{ selectedDevices.length }} 台设备\r\n          </div>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"batchDialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"submitBatchInbound\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport { Component, Vue } from 'vue-property-decorator'\r\nimport HeadLable from '@/components/HeadLable/index.vue'\r\nimport {\r\n  getDevicePage,\r\n  InboundById,\r\n  batchInbound \r\n} from '@/api/warehouse.ts'\r\nimport InputAutoComplete from '@/components/InputAutoComplete/index.vue'\r\nimport Empty from '@/components/Empty/index.vue'\r\n\r\n@Component({\r\n  name: 'inbound',\r\n  components: {\r\n    HeadLable,\r\n    InputAutoComplete,\r\n    Empty\r\n  }\r\n})\r\nexport default class extends Vue {\r\n  private equipmentName: any = ''\r\n  private counts: number = 0\r\n  private page: number = 1\r\n  private pageSize: number = 10\r\n  private tableData: any[] = []\r\n  private selectedDevices: any[] = [] // 存储选中的设备\r\n  private equipmentType = ''\r\n  private status = 1\r\n  private isSearch: boolean = false\r\n  private dialogVisible = false; // 控制单个入库弹窗显示\r\n  private batchDialogVisible = false; // 控制批量入库弹窗显示\r\n  private currentDeviceId = '';  // 当前操作的设备ID\r\n  \r\n  // 单个入库表单\r\n  private inboundForm = {\r\n    areald: '',                  // 区域ID\r\n    deviceId: '',               // 设备ID\r\n    inboundBy: '',              // 入库操作人\r\n    inspectionDate: '',         // 质检日期\r\n    inspectionResult: '',       // 质检结果\r\n    storageLocation: '',        // 货架位置\r\n    warehouseId: ''             // 仓库ID\r\n  };\r\n\r\n  // 批量入库表单\r\n  private batchInboundForm = {\r\n    deviceIds: [] as string[],  // 设备ID数组\r\n    inboundBy: '',              // 入库操作人\r\n    inspectionDate: '',         // 质检日期\r\n    inspectionResult: '',       // 质检结果\r\n    storageLocation: '',        // 货架位置\r\n    warehouseId: ''             // 仓库ID\r\n  };\r\n\r\n  created() {\r\n    this.init()\r\n  }\r\n\r\n  initProp(val) {\r\n    this.equipmentName = val\r\n    this.initFun()\r\n  }\r\n\r\n  initFun() {\r\n    this.page = 1\r\n    this.init()\r\n  }\r\n\r\n  private async init(isSearch?) {\r\n    this.isSearch = isSearch\r\n    await getDevicePage({\r\n      page: this.page,  \r\n      pageSize: this.pageSize,\r\n      equipmentName: this.equipmentName || undefined,\r\n      equipmentType: this.equipmentType || undefined,\r\n      status: 1\r\n    })\r\n      .then(res => {\r\n        if (res.data.code === 1) {\r\n          this.tableData = res.data && res.data.data && res.data.data.records\r\n          this.counts = Number(res.data.data.total)\r\n        }\r\n      })\r\n      .catch(err => {\r\n        this.$message.error('请求出错了：' + err.message)\r\n      })\r\n  }\r\n\r\n  // 表格选择项变化\r\n  private handleSelectionChange(val: any[]) {\r\n    this.selectedDevices = val;\r\n  }\r\n\r\n  // 显示批量入库对话框\r\n  private showBatchInboundDialog() {\r\n    if (this.selectedDevices.length === 0) {\r\n      this.$message.warning('请至少选择一台设备');\r\n      return;\r\n    }\r\n    this.batchInboundForm.deviceIds = this.selectedDevices.map(device => device.deviceId);\r\n    this.batchDialogVisible = true;\r\n  }\r\n\r\n  // 单个设备入库\r\n  private async deviceInbound(deviceId: string) {\r\n    this.currentDeviceId = deviceId;\r\n    this.inboundForm.deviceId = deviceId;\r\n    this.dialogVisible = true;\r\n  }\r\n\r\n  // 提交单个入库\r\n  private async submitInbound() {\r\n    try {\r\n      // 必填字段验证\r\n      if (!this.inboundForm.inboundBy || !this.inboundForm.inspectionDate ||\r\n        !this.inboundForm.inspectionResult || !this.inboundForm.storageLocation) {\r\n        this.$message.warning('请填写完整入库信息');\r\n        return;\r\n      }\r\n\r\n      const res = await InboundById({\r\n        ...this.inboundForm,\r\n        inspectionDate: this.inboundForm.inspectionDate + ' 00:00:00'\r\n      });\r\n\r\n      if (res.data.code === 1) {\r\n        this.$message.success('入库成功');\r\n        this.dialogVisible = false;\r\n        this.init();\r\n      }\r\n    } catch (err) {\r\n      this.$message.error('操作失败: ' + (err.message || '未知错误'));\r\n    }\r\n  }\r\n\r\n  // 提交批量入库\r\n  private async submitBatchInbound() {\r\n    try {\r\n      // 必填字段验证\r\n      if (!this.batchInboundForm.inboundBy || !this.batchInboundForm.inspectionDate ||\r\n        !this.batchInboundForm.inspectionResult || !this.batchInboundForm.storageLocation) {\r\n        this.$message.warning('请填写完整入库信息');\r\n        return;\r\n      }\r\n\r\n      const params = {\r\n        ...this.batchInboundForm,\r\n        deviceIds: this.selectedDevices.map(device => device.deviceId),\r\n        inspectionDate: this.batchInboundForm.inspectionDate + ' 00:00:00'\r\n      };\r\n\r\n      const res = await batchInbound(params);\r\n\r\n      if (res.data.code === 1) {\r\n        this.$message.success(`成功入库 ${this.selectedDevices.length} 台设备`);\r\n        this.batchDialogVisible = false;\r\n        this.selectedDevices = [];\r\n        this.init();\r\n      }\r\n    } catch (err) {\r\n      this.$message.error('批量入库失败: ' + (err.message || '未知错误'));\r\n    }\r\n  }\r\n\r\n  // 重置表单\r\n  private resetForm() {\r\n    this.inboundForm = {\r\n      areald: '',\r\n      deviceId: this.currentDeviceId,\r\n      inboundBy: '',\r\n      inspectionDate: '',\r\n      inspectionResult: '',\r\n      storageLocation: '',\r\n      warehouseId: ''\r\n    };\r\n  }\r\n\r\n  private handleSizeChange(val: any) {\r\n    this.pageSize = val\r\n    this.init()\r\n  }\r\n\r\n  private handleCurrentChange(val: any) {\r\n    this.page = val\r\n    this.init()\r\n  }\r\n}\r\n</script>\r\n<style lang=\"scss\">\r\n.el-table-column--selection .cell {\r\n  padding-left: 10px;\r\n}\r\n</style>\r\n<style lang=\"scss\" scoped>\r\n.dashboard {\r\n  &-container {\r\n    margin: 30px;\r\n\r\n    .container {\r\n      background: #fff;\r\n      position: relative;\r\n      z-index: 1;\r\n      padding: 30px 28px;\r\n      border-radius: 4px;\r\n\r\n      //查询黑色按钮样式\r\n      .normal-btn {\r\n        background: #333333;\r\n        color: white;\r\n        margin-left: 20px;\r\n        \r\n        // 禁用状态样式\r\n        &.is-disabled {\r\n          background: #cccccc;\r\n          color: #999999;\r\n          cursor: not-allowed;\r\n        }\r\n      }\r\n\r\n      .tableBar {\r\n        margin-bottom: 20px;\r\n\r\n        .tableLab {\r\n          display: inline-block;\r\n          float: right;\r\n\r\n          span {\r\n            cursor: pointer;\r\n            display: inline-block;\r\n            font-size: 14px;\r\n            padding: 0 20px;\r\n            color: $gray-2;\r\n          }\r\n        }\r\n      }\r\n\r\n      .tableBox {\r\n        width: 100%;\r\n        border: 1px solid $gray-5;\r\n        border-bottom: 0;\r\n      }\r\n\r\n      .pageList {\r\n        text-align: center;\r\n        margin-top: 30px;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.el-dialog {\r\n  .el-form-item {\r\n    margin-bottom: 22px;\r\n  }\r\n\r\n  .el-date-editor,\r\n  .el-textarea {\r\n    width: 100%;\r\n  }\r\n}\r\n</style>\r\n"]}]}